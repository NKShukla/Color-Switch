<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ShareScreenController">
    <children>
        <ImageView fitHeight="500.0" fitWidth="500.0" pickOnBounds="true">
            <image>
                <Image url="@Images/bg.jpg" />
            </image>
        </ImageView>
        <ImageView fx:id="backImage" fitHeight="50.0" fitWidth="115.0" layoutX="14.0" layoutY="14.0" onMouseClicked="#backHome" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@Images/back.png" />
            </image>
        </ImageView>
      <ImageView fitHeight="125.0" fitWidth="179.0" layoutX="64.0" layoutY="114.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Images/fb.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="107.0" fitWidth="125.0" layoutX="73.0" layoutY="300.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Images/insta.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="135.0" fitWidth="150.0" layoutX="271.0" layoutY="104.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Images/twitter.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="107.0" fitWidth="150.0" layoutX="285.0" layoutY="300.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Images/whatsapp.png" />
         </image>
      </ImageView>
      <Hyperlink layoutX="83.0" layoutY="235.0" onMouseClicked="#openFB" text="FaceBook" textFill="RED">
         <font>
            <Font name="Palatino Linotype Bold" size="17.0" />
         </font>
      </Hyperlink>
      <Hyperlink layoutX="304.0" layoutY="235.0" onMouseClicked="#openTwitter" text="Twitter" textFill="RED">
         <font>
            <Font name="Palatino Linotype Bold" size="17.0" />
         </font>
      </Hyperlink>
      <Hyperlink layoutX="291.0" layoutY="417.0" onMouseClicked="#openWhatsApp" text="WhatsApp" textFill="RED">
         <font>
            <Font name="Palatino Linotype Bold" size="17.0" />
         </font>
      </Hyperlink>
      <Hyperlink layoutX="83.0" layoutY="417.0" onMouseClicked="#openInsta" text="Instagram" textFill="RED">
         <font>
            <Font name="Palatino Linotype Bold" size="17.0" />
         </font>
      </Hyperlink>
    </children>

</AnchorPane>
